---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	
	<title type="text" xml:lang="en">{{ site.root_desc }}</title>
	<link type="application/atom+xml" href="http://bingjie.me/feed/" rel="self"/>
 	<link type="text" href="http://bingjie.me" rel="alternate"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://bingjie.me</id>
	<author>
		<name>Bingjie</name>
	</author>
	<rights>Copyright (c) 2015-2022, Zhai Bingjie; all rights reserved.</rights>
	
	{% for post in site.posts limit:20 %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://bingjie.me{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://bingjie.me{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>